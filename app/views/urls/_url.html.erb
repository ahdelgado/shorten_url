<% cache url do %>
  <tr>
    <td>
      <center><%= link_to url.long_url, url %></center>
    </td>
    <td>
      <center><%= link_to url.base_url + url.display_url, url %></center>
    </td>
    <td>
      <center>
        <%= link_to 'Edit', edit_url_path(url),
                    method: :get,
                    class: 'edit' %>
        <%= link_to 'Delete', url,
                    method: :delete,
                    data: { confirm: "Are you sure you want to delete this URL?" },
                    class: 'delete' %>
      </center>
    </td>
  </tr>
<% end %>