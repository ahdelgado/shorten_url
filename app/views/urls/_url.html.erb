<tr>
  <td>
    <center><%= link_to url.long_url, url %></center>
  </td>
  <td>
    <center><%= link_to url.base_url + url.short_url[0..6], url %></center>
  </td>
  <td>
    <center>
      <%= link_to 'Delete', url,
                      method: :delete,
                      data: { confirm: "Are you sure you want to delete this URL?" },
                      class: "delete" %>
    </center>
  </td>
</tr>