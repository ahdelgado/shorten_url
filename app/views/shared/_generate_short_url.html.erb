<div class='row'>
  <div class='col-md-6 col-md-offset-3'>
    <%= form_for(@url) do |f| %>
      <%= render 'shared/error_messages' %>
      <%= f.text_field :long_url, class: 'form-control', placeholder: placeholder %>
      <%= f.submit 'Generate a Short URL', class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>